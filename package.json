{
  "name": "carriage-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@typescript-eslint/parser": "^5.62.0",
    "typescript": "catalog:"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8"
  },
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "start:frontend": "cd frontend && pnpm run start",
    "start:server": "cd server && pnpm run dev",
    "build:frontend": "cd frontend && pnpm run build",
    "build:server": "cd server && pnpm run build",
    "quiet-lint": "pnpm eslint . --ext .js,.jsx,.ts,.tsx --fix --quiet",
    "lint": "pnpm eslint . --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx --fix",
    "format": "pnpm prettier --write \"**/*.{ts,js,tsx,jsx,css}\"",
    "format:check": "pnpm prettier --check \"**/*.{ts,js,tsx,jsx,css}\"",
    "type-check": "cd frontend && pnpm run type-check && cd ../server && pnpm run type-check",
    "test:server": "cd server && pnpm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cornell-dti/carriage-web.git"
  },
