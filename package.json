{
  "name": "carriage-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "heroku-postbuild": "npm run build:frontend && npm run build:server",
    "start:frontend": "cd frontend && npm run start",
    "start:server": "cd server && npm run start",
    "build:frontend": "cd frontend && npm run build",
    "build:server": "cd server && npm run tsc",
    "install:frontend": "cd frontend && npm install",
    "install:server": "cd server && npm install",
    "postinstall": "npm run install:frontend && npm run install:server",
    "quiet-lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --fix --quiet",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --fix",
    "format": "npx prettier --write '**/*.{ts,js,tsx,jsx,css,json}'",
    "format:check": "npx prettier --check '**/*.{ts,js,tsx,jsx,css,json}'",
    "type-check": "cd frontend && npm run type-check && cd ../server && npm run type-check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cornell-dti/carriage-web.git"
  },
